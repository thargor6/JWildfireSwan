{
  "name": "application",
  "version": "1.0.0",
  "main": "./main.js",
  "license": "GPL-3.0",
  "description": "Electron wrapper for 'application'",
  "dependencies": {
    "decompress": "^4.2.1",
    "electron-log": "^4.4.7",
    "get-port": "^5.1.1",
    "minimal-request-promise": "^1.5.0"
  },
  "scripts": {
    "package": "electron-packager ./ --icon=icon.ico",
